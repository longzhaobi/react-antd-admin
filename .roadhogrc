{
  "entry": "src/index.js",
  "disableCSSModules": false,
  "publicPath": "/",
  "theme": {
    "@primary-color": "#0099CC",
    "@link-color": "#0099CC",
    "@border-radius-base": "2px",
    "@font-size-base": "13px",
    "@line-height-base": "1.2"
  },
  "autoprefixer": null,
  "proxy": {
    "/api": {
      "target": "http://localhost:8188",
      "changeOrigin": true,
      "pathRewrite": { "^/api" : "/api" }
    }
  },
  "extraBabelPlugins": [
    "transform-runtime",
    ["import", { "libraryName": "antd", "style": true }]
  ],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime"
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime"
      ]
    }
  }
}
